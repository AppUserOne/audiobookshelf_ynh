{
    "name": "Audiobookshelf",
    "id": "audiobookshelf",
    "packaging_format": 1,
    "description": {
        "en": "Audiobook server for managing and playing your audiobooks",
        "fr": "Expliquez en *quelques* (10~15) mots l'utilité de l'app ou ce qu'elle fait (l'objectif est de donner une idée grossière pour des utilisateurs qui naviguent dans un catalogue de 100+ apps)"
    },
    "version": "1.7.2~ynh1",
    "url": "https://audiobookshelf.org/",
    "upstream": {
        "license": "free",
        "website": "https://audiobookshelf.org/",
        "demo": "https://audiobookshelf.org/",
        "admindoc": "https://yunohost.org/packaging_apps",
        "userdoc": "https://www.audiobookshelf.org/docs",
        "code": "https://github.com/advplyr/audiobookshelf"
    },
    "license": "free",
    "maintainer": {
        "name": "Navan Chauhan",
        "email": "audiobookshelf_ynh@navan.email"
    },
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
